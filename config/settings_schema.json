  {
    "name": "Featured product",
    "settings": [
      {
        "type": "product",
        "id": "featured-product-main",
        "label": "Product",
        "info": "Choose the product that will be used for the Product Focus and Product Goal sections"
      },
      {
        "type": "paragraph",
        "content": "Select your featured product sections in the order you'd like them to appear on the product page."
      },
      {
        "type": "select",
        "id": "product_module_1",
        "options": [
          {
            "value": "none",
            "label": "None"
          },
          {
            "value": "module-product-focus",
            "label": "Product focus"
          },
          {
            "value": "module-technical-specifications",
            "label": "Technical specifications"
          }
        ]
      },
      {
        "type": "select",
        "id": "product_module_2",
        "options": [
          {
            "value": "none",
            "label": "None"
          },
          {
            "value": "module-product-focus",
            "label": "Product focus"
          },
          {
            "value": "module-technical-specifications",
            "label": "Technical specifications"
          }
        ]
      }
    ]
  },
  {
    "name": "Product goal",
    "settings": [
      {
        "type": "paragraph",
        "content": "[Learn more about taking pre-orders.](https:\/\/help.shopify.com\/themes\/customization\/products\/allow-pre-orders#authorizing-payment-for-pre-orders)"
      },
      {
        "type": "paragraph",
        "content": "The Product goal will be applied to your Featured product. You can choose your Featured product in \"Featured product\" under \"General settings\""
      },
      {
        "type": "checkbox",
        "id": "product-sale-goal-use",
        "label": "Enable",
        "default": false
      },
      {
        "type": "select",
        "id": "product-sale-goal-style",
        "label": "Background style",
        "options": [
          {
            "value": "default",
            "label": "Default"
          },
          {
            "value": "light",
            "label": "Light"
          },
          {
            "value": "color",
            "label": "Color"
          },
          {
            "value": "gradient",
            "label": "Gradient"
          }
        ],
        "default": "gradient"
      },
      {
        "type": "text",
        "id": "product-sale-goal-button",
        "label": "Button text",
        "info": "This text will change the button text on the home module, and the add to cart button on the Featured product's page.",
        "default": "Pre-order"
      },
      {
        "type": "text",
        "id": "product-sale-goal-estimated-delivery-date",
        "label": "Estimated delivery text",
        "info": "Write a message to let your customers know when they should expect a delivery.",
        "default": "Estimated delivery: April"
      },
      {
        "type": "header",
        "content": "Inventory"
      },
      {
        "type": "text",
        "id": "product-goal-starting-quantity",
        "label": "Starting quantity",
        "info": "To start your goal's progress at 0%, enter a number equal to your Featured product's current inventory.  If you have already made pre-sales, enter a number higher than your Featured product's inventory to account for the progress you've made."
      },
      {
        "type": "text",
        "id": "product-goal",
        "label": "Goal quantity",
        "info": "This value represents the number of sales\/pre-orders you'd like to make before funding is completed."
      },
      {
        "type": "checkbox",
        "id": "product-goal-use-quantity",
        "label": "Show product goal as quantity",
        "info": "This option will change the product goal to show units sold instead of money raised.",
        "default": false
      },
      {
        "type": "header",
        "content": "Goal End"
      },
      {
        "type": "text",
        "id": "product-sale-goal-date-time",
        "label": "End date and time",
        "info": "The mm\/dd\/yyyy hh:mm format is required. For example, June 21st 2017 at 3pm would be 06\/21\/2017 15:00",
        "default": "06\/21\/2017 15:00"
      }
    ]
  },
