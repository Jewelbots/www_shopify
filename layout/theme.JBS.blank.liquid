  <style>
	@import url('https://fonts.googleapis.com/css?family=Nunito:300,300i,700,700i%7CQuicksand:700');

  html{
		font-family:  'Nunito', sans-serif;
		font-weight:300;
		color: black;
	}
	h1,
	h2,
	h3,
	h4,
	h5,
	h6 {
		font-family: 'Quicksand', sans-serif;
		font-weight:400;
		color: #e1146e;
   }

  .inner-welcome img{
  	max-width: 450px;
  	height: auto;
  }
	.inner-welcome h2{
  	font-weight: bold;
  	font-size: 27px;

  }
	.inner-welcome h3{
  	font-weight: bold;
  	font-size: 20px;
  }
	.inner-welcome h4{
		line-height: 3px;
	}
	.setup h4{

		padding-top: 15px;
	}

  .inner-welcome{
  	display: block;
  	width: 100%;
		background-color:white;
  	right:0;
  	top: 0;
  	min-height: 700px;
		color: black;
  }
  .inner-welcome.copy{
  	background-color: white;
  }
  .jbot_onboarding img{
    width: 100%;
    max-width:1000px;
  }

	.sample_code {
		width: 70%;
		background-color: #fbd2e4;
		color: #5a4e53;
		font-size: 20px;
		margin-right: 10px;
		padding: 0 30px 10px 30px;
		white-space: pre-wrap;
		font-family: monospace;
	}
	.led_image img{
		width: 65%;
	  max-width: 700px;
	  display: block;
	}
	@media (min-width: 767px) {
		.back-mobile{
			display:none;
		}
  </style>

